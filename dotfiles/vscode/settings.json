{
    "editor.fontFamily": "'Fira Code'",
    "editor.fontLigatures": true,
    "editor.rulers": [
        80,
        100,
        120
    ],
    "gitlens.keymap": "alternate",
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true
    },
    "window.zoomLevel": 0,
    "files.insertFinalNewline": true,
    "files.associations": {
        "Jenkinsfile*": "jenkins"
    },
    // https://github.com/VSpaceCode/VSpaceCode/blob/master/settings.json
    "vim.leader": "<space>",
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<leader>",
                "<tab>"
            ],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.openNextRecentlyUsedEditorInGroup",
                    "args": []
                }
            ]
        },
        {
            "before": [
                "leader",
                "j",
                "j"
            ],
            "after": [
                "leader",
                "leader",
                "s"
            ]
        },
    ],
    "vim.easymotion": true,
    "terminal.integrated.drawBoldTextInBrightColors": false,
    "trailing-spaces.trimOnSave": true,
    "[groovy]": {
        "editor.rulers": [
            120
        ]
    },
    "gitlens.views.fileHistory.enabled": false,
    "gitlens.views.lineHistory.enabled": false,
    "search.usePCRE2": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.iconTheme": "material-icon-theme",
    "eslint.alwaysShowStatus": true,
    "eslint.autoFixOnSave": true,
    "[plaintext]": {},
    "peacock.favoriteColors": [
        {
            "name": "Angular Red",
            "value": "#b52e31"
        },
        {
            "name": "Auth0 Orange",
            "value": "#eb5424"
        },
        {
            "name": "Azure Blue",
            "value": "#007fff"
        },
        {
            "name": "C# Purple",
            "value": "#68217A"
        },
        {
            "name": "Gatsby Purple",
            "value": "#639"
        },
        {
            "name": "Go Cyan",
            "value": "#5dc9e2"
        },
        {
            "name": "Java Blue-Gray",
            "value": "#557c9b"
        },
        {
            "name": "JavaScript Yellow",
            "value": "#f9e64f"
        },
        {
            "name": "Mandalorian Blue",
            "value": "#1857a4"
        },
        {
            "name": "Node Green",
            "value": "#215732"
        },
        {
            "name": "React Blue",
            "value": "#00b3e6"
        },
        {
            "name": "Something Different",
            "value": "#832561"
        },
        {
            "name": "Vue Green",
            "value": "#42b883"
        }
    ],
    "java.codeGeneration.generateComments": true,
    "java.autobuild.enabled": false,
    "java.saveActions.organizeImports": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "liveshare.audio.startCallOnShare": true,
    "task.slowProviderWarning": [
        "npm"
    ],
    "explorer.confirmDragAndDrop": false,
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "telemetry.enableTelemetry": false,
    "files.watcherExclude": {
        "**/vendor/**": true
    },
    "files.exclude": {
        "**/.vscode": true,
        "**/node_modules": true,
        "**/vendor": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "jsonnet.executablePath": "true",
    "go.inferGopath": true,
    "go.formatTool": "gofmt",
    "go.buildOnSave": "off",
    "go.coverOnTestPackage": false,
    "go.installDependenciesWhenBuilding": false,
    "go.lintOnSave": "file",
    "go.vetOnSave": "off",
    "go.liveErrors": {
        "enabled": true,
        "delay": 1000
    },
    "go.autocompleteUnimportedPackages": true,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "autogencc.patterns.header": [
        "^(\\/\\/|#+) Code generated( .*)? DO NOT EDIT\\.?$",
        "\\/\\/ This file has been autogenerated by .*"
    ],
    "go.languageServerExperimentalFeatures": {
        "diagnostics": false,
        "documentLink": true
    }
}
